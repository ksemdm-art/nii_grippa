{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "OrderDate": {
      "type": "date",
      "required": true
    },
    "OrderStatus": {
      "type": "boolean",
      "required": true
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service"
    },
    "OrderName": {
      "type": "string",
      "required": true
    },
    "orderFile": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "order"
    }
  }
}
